{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/registration",
      "dest": "/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/registration/login",
      "dest": "/server.js",
      "methods": ["POST"]
    }
  ]
}
